<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
   <class name = "com.subash.HibernateEx2.Employee" table ="Employee">
      
           
      <id name = "eid" type = "int" column = "employeeid">
         <generator class="assigned"/>
      </id>
      
      <property name = "ename" column = "employeename" type = "string"/>
      <property name = "ecity" column = "employeecity" type = "string"/>
      
   </class>
</hibernate-mapping>