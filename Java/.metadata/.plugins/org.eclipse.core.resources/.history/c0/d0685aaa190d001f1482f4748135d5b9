package com.day6;

import java.io.File;

class file{
	public void print() {
		for(int i = 0 ; i < 10 ; i++) {
			System.out.println(i);
		}
	}
}

class ThreadA extends Thread{
	File f = new File();
	ThreadA(File f){
		this.f = f;
	}
	public void run() {
		f.print();
	}
}

class ThreadB extends Thread{
	File f = new File();
	ThreadB(File f){
		this.f = f;
	}
	public void run() {
		f.print();
	}
}

public class SyncEx {

	public static void main(String[] args) {
		ThreadA t1 = new ThreadA(f);
		ThreadB t2 = new ThreadB(f);
	}

}
