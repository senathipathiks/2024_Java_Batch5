LinkedList<Employee> list = (LinkedList<Employee>) request.getAttribute("list");

out.println("<h1 align=center>Employee Details</h1><hr>");

out.println("<center><table border=1><tr><th>Employee ID</th><th>Employee Name</th><th>Employee Designation</th><th>Employee Department</th></tr>");
for(Employee e : list){
	out.print("<tr>");
	out.print("<td>"+e.getEid()+"</td><td>"+e.getEname()+"</td><td>"+e.getEdes()+"</td><td>"+e.getEdep()+"</td></tr>");
}
out.println("</table></center>");
