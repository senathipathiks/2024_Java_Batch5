package com.day2;

import java.util.Scanner;

public class SumAvg {

	public static void main(String[] args) {
		int arr[] = new int[10];
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter 10 numbers: ");
		int n = sc.nextInt();
		
		for(int i = 0 ; i<n; i++) {
			arr[i]=n;
		}
		
		System.out.println("The numbers are:");
		for(int i=0; i<n ; i++) {
			System.out.println(arr[i]);
		}

	}

}
