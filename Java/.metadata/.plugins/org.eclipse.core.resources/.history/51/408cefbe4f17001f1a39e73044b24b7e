package com.stu.bean;

public class Student {
	String sid;
	String sname;
	String scity;
	
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Student(String sid, String sname, String scity) {
		super();
		this.sid = sid;
		this.sname = sname;
		this.scity = scity;
	}

	
	
}
